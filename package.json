{
  "name": "@sentry0/qemu-hook-manager",
  "version": "0.0.4-alpha",
  "description": "A simple QEMU hook manager.",
  "main": "dist/index.js",
  "scripts": {
    "qemu-hook-manager": "node index.js",
    "test": "echo \"No tests specified\"",
    "lint": "npx eslint *.js",
    "check-format": "npx prettier --check \"*.js\"",
    "format": "npx prettier --write \"*.js\"",
    "watch": "rimraf dist && esbuild ./qemu_hook_skeleton/qemu.sh ./qemu_hook_skeleton/**/**/**/* --loader:.sh=copy --minify --platform=node --outdir=dist index.js src/libs/*.js --watch",
    "build": "rimraf dist && esbuild ./qemu_hook_skeleton/* ./qemu_hook_skeleton/**/**/**/* --loader:.sh=copy --minify --platform=node --outdir=dist index.js src/libs/*.js",
    "prepare": "husky"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "envsub": "^4.1.0",
    "prompts": "^2.4.2",
    "sanitize-filename": "^1.6.3",
    "winston": "^3.17.0",
    "winston-console-format": "^1.0.8",
    "winston-syslog": "^2.7.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/preset-react": "^7.26.3",
    "@types/yargs": "^17.0.33",
    "dotenv": "^16.4.7",
    "esbuild": "^0.24.2",
    "esbuild-envfile-plugin": "^1.0.7",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1"
  },
  "lint-staged": {
    "*.js": "prettier --check --ignore-unknown"
  },
  "bin": {
    "qemu-hook-manager": "dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "keywords": [
    "QEMU",
    "hook",
    "kvm",
    "hardware",
    "passthrough",
    "manager"
  ],
  "author": "Slack Day Studio",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slackdaystudio/qemu-hook-manager.git"
  },
  "bugs": {
    "url": "https://github.com/slackdaystudio/qemu-hook-manager/issues"
  },
  "homepage": "https://github.com/slackdaystudio/qemu-hook-manager#readme"
}
